{
  "extends": [
    "airbnb",
    "standard-with-typescript",
    "eslint-config-prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "overrides": [
  ],
  "rules": {
    "max-len": [
      2,
      100
    ],
    "@typescript-eslint/no-unused-vars": 2
  }
}
